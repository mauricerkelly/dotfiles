"*":
  "exception-reporting":
    userId: "5392f155-a911-a1c0-cdd2-604cd7bf4ef0"
  "release-notes":
    viewedVersion: "0.95.0"
  welcome:
    showOnStartup: false
  metrics:
    userId: "d96913c83b39b64e17220206b7c618adf83dbb33"
  editor:
    fontSize: 12
    showIndentGuide: true
    preferredLineLength: 110
    tabLength: 4
    softWrap: true
    softWrapAtPreferredLineLength: true
    invisibles: {}
  core:
    ignoredNames: [
      ".git"
      ".svn"
      ".DS_Store"
    ]
    themes: [
      "atom-light-ui"
      "tomorrow"
    ]
    disabledPackages: [
      "go-plus"
      "spell-check"
    ]
  rspec:
    command: "bundle exec rspec"
  "cucumber-runner":
    command: "bundle exec cucumber"
  "file-types":
    $debug: true
    "^Podfile$": "source.ruby"
  "autocomplete-plus":
    fileBlacklist: [
      ".md"
    ]
  "spell-check": {}
  minimap:
    plugins:
      "git-diff": true
      selection: true
      "find-and-replace": true
      "highlight-selected": true
  "atom-beautify":
    _analyticsUserId: "67d3667b-2f2f-44fd-8841-caf8667ba968"
  "markdown-writer": {}
".basic.html.text":
  editor:
    preferredLineLength: 110
    tabLength: 2
".erb.html.text":
  editor:
    tabLength: 2
".coffee.source":
  editor:
    tabLength: 2
".shell.source":
  editor:
    tabLength: 2
".ruby.source":
  editor:
    tabLength: 2
".litcoffee.source":
  editor:
    tabLength: 2
".diff.source":
  editor:
    preferredLineLength: 110
